<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="858.0" prefWidth="1306.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.enrollmentsystem.controllers.dashboard.enrollment.EnrollmentController">
   <children>
      <ScrollPane fitToWidth="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <content>
            <VBox prefHeight="563.0" prefWidth="778.0">
               <children>
                  <Label fx:id="title" text="/Grade 11">
                     <graphic>
                        <Label text="Add Student">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                     </graphic>
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <VBox prefHeight="570.0" prefWidth="840.0">
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="738.0">
                           <children>
                              <Label text="Enrolled Student List">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </Label>
                              <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                              <HBox fx:id="searchbarContainer" alignment="CENTER" prefHeight="50.0" prefWidth="266.0" />
                           </children>
                        </HBox>
                        <TableView fx:id="enrollmentTable" prefHeight="200.0" VBox.vgrow="ALWAYS">
                           <columns>
                              <TableColumn fx:id="lrnCol" editable="false" minWidth="50.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="LRN" />
                              <TableColumn fx:id="lastnameCol" editable="false" minWidth="50.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="LAST NAME" />
                              <TableColumn fx:id="firstnameCol" editable="false" minWidth="50.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="FIRST NAME" />
                              <TableColumn fx:id="middlenameCol" editable="false" minWidth="50.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="MIDDLE NAME" />
                              <TableColumn fx:id="ayCol" editable="false" minWidth="30.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="A.Y." />
                              <TableColumn fx:id="gradeCol" editable="false" minWidth="30.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="GRADE" />
                              <TableColumn fx:id="termCol" editable="false" minWidth="30.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="TERM" />
                              <TableColumn fx:id="trackCol" editable="false" minWidth="30.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="TRACK" />
                              <TableColumn fx:id="strandCol" editable="false" minWidth="30.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="STRAND" />
                              <TableColumn fx:id="sectionCol" editable="false" minWidth="30.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="SECTION" />
                              <TableColumn fx:id="statusCol" editable="false" minWidth="30.0" prefWidth="-1.0" sortable="false" styleClass="table-columns" text="STATUS" />
                              <TableColumn fx:id="actionCol" editable="false" minWidth="100.0" prefWidth="75.0" sortable="false" styleClass="table-columns" text="ACTION" />
                           </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                     </children>
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding>
